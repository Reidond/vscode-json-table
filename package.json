{
    "name": "vscode-json-table",
    "displayName": "JSON Table",
    "description": "Edit your json files using table",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.59.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:vscode-json-table.helloWorld"
    ],
    "main": "./out/extension.js",
    "contributes": {},
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "lint": "eslint src --ext ts,tsx,js,jsx --quiet --fix",
        "test": "node ./out/test/run-test.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "14.x",
        "@types/vscode": "^1.59.0",
        "@typescript-eslint/eslint-plugin": "^4.29.1",
        "@typescript-eslint/parser": "^4.29.1",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.24.0",
        "glob": "^7.1.7",
        "husky": "^7.0.1",
        "lint-staged": "^11.1.2",
        "mocha": "^8.4.0",
        "prettier": "^2.3.2",
        "typescript": "^4.3.2",
        "vscode-test": "^1.5.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint --fix"
        ]
    }
}
